---
import MatrixItemDetails from '../../components/MatrixItemDetails.astro';
import { loadStaticPaths } from '../../components/load-static-paths';

export async function getStaticPaths() { 
	const section = "user-need-contexts";
	const paths = await loadStaticPaths(section);
	return paths;
}

const {id} = Astro.params;
---
<MatrixItemDetails id={id} type="User Need Context" section="user-need-contexts"></MatrixItemDetails>

